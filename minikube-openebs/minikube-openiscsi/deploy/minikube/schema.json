{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "checksums": {
        "type": "object",
        "properties": {
          "darwin": {
            "type": "string",
            "pattern": "^[A-Fa-f0-9]{64}$"
          },
          "linux": {
            "type": "string",
            "pattern": "^[A-Fa-f0-9]{64}$"
          },
          "windows": {
            "type": "string",
            "pattern": "^[A-Fa-f0-9]{64}$"
          }
        },
        "required": ["darwin", "linux", "windows"]
      }
    },
    "required": [
      "name"
    ]
  }
}
